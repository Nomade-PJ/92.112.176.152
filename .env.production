# Configurações do servidor
NODE_ENV=production
PORT=3000
USE_SUPABASE=true

# URLs da aplicação
VITE_APP_URL=https://seu-dominio-paulocell.com
VITE_API_URL=/api

# Configurações do Supabase (valores reais do projeto)
VITE_SUPABASE_URL=https://seu-projeto-supabase.supabase.co
VITE_SUPABASE_ANON_KEY=sua-chave-anonima-real-aqui
SUPABASE_SERVICE_ROLE_KEY=sua-chave-service-role-real-aqui

# Configurações de autenticação
